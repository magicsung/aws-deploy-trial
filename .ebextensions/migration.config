option_settings:
container_commands:
    database_migration:
        leader_only: true
        commands: 
            setvars: 
                command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
            dbmigrate: 
                command: npm run dbmigrate