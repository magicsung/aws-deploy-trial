option_settings:
container_commands:
    setvars: 
        leader_only: true
        command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
    database_migration:
        leader_only: true
        commands: npm run dbmigrate